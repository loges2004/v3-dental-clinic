{
  "version": 2,
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "v3dentalclinic.vercel.app" }
      ],
      "destination": "https://v3dentalclinic.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}